

<table style="width:100%">
    <thead class="thead">
        <tr>
            <th>ID</th>
            <th>Name</th> 
            <th>Status</th>
            <th>Email</th>
            <th>Action</th>
        </tr>
    </thead>
    <tbody>
        <% for(var i = 0; i<users.length; i++) {%>
        <tr>
            <td><%=i+1%></td>
            <td><%=users[i].name%></td> 
            <td><%=users[i].status%></td> 
            <td><%=users[i].email%></td>
            <td>
                <a href ='/update-user?id=<%=user[i]._id%>' class='upd'>
                   <span>Update</span>
                </a>
                <a href ='#' class='del'>
                    <span>Delete</span>
                 </a>
            </td>
        </tr>
        <% }%>
    </tbody>
  </table>
  